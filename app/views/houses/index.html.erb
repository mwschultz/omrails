<p id="notice"><%= notice %></p>

<h1>Houses</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>St address</th>
      <th>House</th>
      <th>Price</th>
      <th>State</th>
      <th>Zip</th>
      <th>County</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @houses.each do |house| %>
      <tr>
        <td><%= house.owner %></td>
        <td><%= house.st_address %></td>
        <td><%= house.house_id %></td>
        <td><%= house.price %></td>
        <td><%= house.state %></td>
        <td><%= house.zip %></td>
        <td><%= house.county %></td>
        <td><%= link_to 'Show', house %></td>
        <td><%= link_to 'Edit', edit_house_path(house) %></td>
        <td><%= link_to 'Destroy', house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New House', new_house_path %>
